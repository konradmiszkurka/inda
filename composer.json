{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.3.11",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/doctrine-fixtures-bundle": "^3.3",
    "fresh/doctrine-enum-bundle": "~6.6",
    "friendsofsymfony/user-bundle": "^2.1",
    "gedmo/doctrine-extensions": "^2.4",
    "knplabs/knp-menu-bundle": "^3.0",
    "lasserafn/php-initial-avatar-generator": "^4.0",
    "stof/doctrine-extensions-bundle": "^1.3",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "4.4.*",
    "symfony/console": "4.4.*",
    "symfony/dotenv": "4.4.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "4.4.*",
    "symfony/orm-pack": "^1.0",
    "symfony/security": "4.4.*",
    "symfony/security-bundle": "4.4.*",
    "symfony/swiftmailer-bundle": "^3.4",
    "omines/datatables-bundle": "^0.4.1",
    "symfony/translation": "^4.4",
    "symfony/twig-bundle": "^4.4",
    "symfony/web-server-bundle": "4.4.*",
    "symfony/yaml": "4.4.*",
    "twig/extensions": "^1.5",
    "twig/twig": "^2",
    "kevinpapst/adminlte-bundle": "^3.0",
    "jms/serializer-bundle": "^3.4",
    "jms/serializer": "3.6.0",
    "sensio/framework-extra-bundle": "^5.4",
    "zircote/swagger-php": "^3.0"
  },
  "require-dev": {
    "codeception/codeception": "^4.1",
    "codeception/module-asserts": "^1.1",
    "codeception/module-cli": "^1.0",
    "codeception/module-db": "^1.0",
    "codeception/module-doctrine2": "^1.0",
    "codeception/module-phpbrowser": "^1.0",
    "codeception/module-rest": "^1.2",
    "codeception/module-sequence": "^1.0",
    "codeception/module-symfony": "^1.0",
    "codeception/module-webdriver": "^1.0",
    "symfony/phpunit-bridge": "^5.1"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "4.4.*"
    }
  }
}
